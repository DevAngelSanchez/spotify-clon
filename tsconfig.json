{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@/components": [
        "src/components/*"
      ],
      "@/icons": [
        "src/icons/*"
      ],
      "@/libs": [
        "src/libs/*"
      ],
      "@/layout": [
        "src/layout/*"
      ],
      "@/pages": [
        "src/pages/*"
      ],
      "@/music": [
        "public/music/*"
      ],
      "@/fonts": [
        "public/fonts/*"
      ],
      "@/helpers": [
        "src/helpers/*"
      ],
      "@/store": [
        "src/store/*"
      ],
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}